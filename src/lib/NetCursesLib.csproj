<Project Sdk="Microsoft.NET.Sdk">

  <!-- Assembly and msbuild properties -->
  <PropertyGroup>
    <TargetFrameworks>
      <!-- .NET Core -->
      netcoreapp1.0;netcoreapp1.1;
      netcoreapp2.0;netcoreapp2.1;
      <!--netcoreapp2.2;netcoreapp3.0;-->
    </TargetFrameworks>
  </PropertyGroup>

  <!-- NuGet properties -->
  <PropertyGroup>
    <PackageId>NetCurses</PackageId>
    <Version>1.0.0</Version>
    <Authors>Piot</Authors>
    <Title>NetCurses</Title>
    <Description>Curses-like</Description>
    <Copyright>Copyright (c) 2015 Piot</Copyright>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RepositoryUrl>https://github.com/Piot/netcurses</RepositoryUrl>
    <PackageTags>ncurses</PackageTags>
  </PropertyGroup>

</Project>
